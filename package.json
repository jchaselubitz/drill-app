{
  "name": "drill-app",
  "main": "expo-router/entry",
  "version": "1.1.4",
  "scripts": {
    "start:expo": "expo start",
    "start:expo:tunnel": "expo start --tunnel",
    "new": "rm -rf node_modules && rm -rf .expo && rm -rf ios && rm -rf android && yarn cache clean && yarn install",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:device": "expo start --dev-client && expo run:ios --device",
    "clean:ios": "npx expo prebuild -p ios --clean",
    "build:ios": "npx expo run:ios --configuration Release",
    "build:device": "npx expo run:ios --configuration Release --device",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@google/genai": "^1.35.0",
    "@nozbe/watermelondb": "^0.28.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.39.3",
    "better-sqlite3": "^12.6.0",
    "dotenv": "^17.2.3",
    "expo": "^55.0.0-preview.5",
    "expo-asset": "~55.0.1",
    "expo-audio": "~55.0.1",
    "expo-background-task": "~55.0.1",
    "expo-constants": "~55.0.1",
    "expo-file-system": "~55.0.1",
    "expo-font": "~55.0.1",
    "expo-glass-effect": "~55.0.2",
    "expo-haptics": "~55.0.1",
    "expo-linking": "~55.0.2",
    "expo-router": "~55.0.0-beta.2",
    "expo-splash-screen": "~55.0.1",
    "expo-status-bar": "~55.0.1",
    "expo-symbols": "~55.0.1",
    "expo-system-ui": "~55.0.1",
    "expo-task-manager": "~55.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.1",
    "react-native-css": "^3.0.1",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.20.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.7.2",
    "zod": "^4.3.5",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@types/react": "~19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~55.0.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "expo-mcp": "~0.2.1",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2"
  },
  "packageManager": "yarn@4.12.0",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@nozbe/watermelondb",
          "@nozbe/simdjson"
        ],
        "listUnknownPackages": false
      }
    }
  }
}

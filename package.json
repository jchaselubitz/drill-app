{
  "name": "drill-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start:expo": "expo start",
    "start:expo:tunnel": "expo start --tunnel",
    "new": "rm -rf node_modules && rm -rf .expo && rm -rf ios && rm -rf android && yarn cache clean && yarn install",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:device": "expo start --dev-client && expo run:ios --device",
    "clean:ios": "npx expo prebuild -p ios --clean",
    "build:ios": "npx expo run:ios --configuration Release",
    "build:device": "npx expo run:ios --configuration Release --device",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@google/genai": "^1.35.0",
    "@nozbe/watermelondb": "^0.28.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.39.3",
    "better-sqlite3": "^12.6.0",
    "dotenv": "^17.2.3",
    "expo": "^54.0.31",
    "expo-audio": "^1.1.1",
    "expo-background-task": "^1.0.10",
    "expo-constants": "^18.0.13",
    "expo-file-system": "^19.0.21",
    "expo-font": "^14.0.10",
    "expo-glass-effect": "^0.1.8",
    "expo-haptics": "^15.0.8",
    "expo-linking": "^8.0.11",
    "expo-router": "^6.0.21",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "^3.0.9",
    "expo-symbols": "^1.0.8",
    "expo-system-ui": "^6.0.9",
    "expo-task-manager": "^14.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-css": "^3.0.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "^0.7.0",
    "zod": "^4.3.5",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2"
